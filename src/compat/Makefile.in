# Makefile for src/compat/
# $Id: Makefile.in,v 1.1 2000/03/22 00:42:57 fabian Exp $

SHELL = /bin/sh
top_srcdir = @top_srcdir@
VPATH = @srcdir@

@SET_MAKE@
INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_SCRIPT = @INSTALL_SCRIPT@

CC = @CC@
LD = @CC@
STRIP = @STRIP@
CFLAGS = @CFLAGS@ -I../.. -I$(top_srcdir) -I$(top_srcdir)/src @DEFS@ $(CFLGS)
CPPFLAGS = @CPPFLAGS@

OBJS = inet_aton.o snprintf.o

doofus:
	@echo ""
	@echo "Let's try this from the right directory..."
	@echo ""
	@cd ../.. && $(MAKE)

depend:
	$(CC) $(CFLAGS) $(CPPFLAGS) -MM $(srcdir)/*.c > .depend

clean:
	@rm -f .depend *.o *~

compat: $(OBJS)

.SUFFIXES:
.SUFFIXES: .c .o .h

.c.o:
	$(CC) $(CFLAGS) $(CPPFLAGS) -c $<

#safety hash
inet_aton.o: inet_aton.c $(top_srcdir)/src/main.h \
$(top_srcdir)/src/eggdrop.h $(top_srcdir)/src/proto.h
